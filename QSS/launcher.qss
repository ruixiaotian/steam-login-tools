QWidget#base_widget {
    /*透明窗体设置*/
    border-radius: 7px;
    background-color: #FFF;
}

QWidget#main_widget {
    /*主要显示窗体设置*/
    border-radius: 16px;
    border: 3px solid #FAF7FF;
    background-color: #E4EEF8;
}

QProgressBar#bar_widget {
    /*进度条设置*/
    text-align: center;
    color: #E4EEF8;
    background-color: #1DBEF5;
    border-style: none;
    border-radius: 6px;
}

QProgressBar::chunk {
    border-radius: 4px;
    background-color: #009DF7;
}