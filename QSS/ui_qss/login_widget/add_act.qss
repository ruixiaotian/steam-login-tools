QWidget#add_account_widget {
    /*设置背景颜色*/
    background-color: #F0F5F9;
    /*设置边距*/
    margin: 10px 15px 10px 15px;
    /*设置圆角*/
    border-radius: 24px;
}

QLineEdit#user_edit {
    /*设置背景颜色*/
    background-color: transparent;
    /*设置内间距*/
    padding: 0 0 0 0;
    margin: 0 0 0 0;
    /*设置圆角*/
    border-radius: 6px;
    /*设置边框*/
    border: 2px solid #1DBEF5;
    color: rgba(89, 107, 143, 90);
}

QLineEdit#password_edit {
    /*设置背景颜色*/
    background-color: transparent;
    /*设置内间距*/
    padding: 0 0 0 0;
    margin: 0 0 0 0;
    /*设置圆角*/
